GET     /                       controllers.Index.main

POST    /search                 controllers.SearchController.search

GET     /monitoring/queries     controllers.SchedulerController.getProcess

POST    /dev/fetch              controllers.FetcherController.fetchRGB

GET     /process/start/:id      controllers.SchedulerController.startProcessing(id: String)

GET     /process/kill/:id       controllers.SchedulerController.cancelProcessing(id: String)